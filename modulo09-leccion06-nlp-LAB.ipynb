    "# Cargar el modelo de SpaCy fuera de la funci칩n para mejorar el rendimiento\n",
    "def limpiar_texto(texto):\n",
    "\n",
    "    texto = contractions.fix(texto)\n",
    "    # Convierte a min칰sculas\n",
    "    texto = texto.lower()\n",
    "    # Elimina signos de puntuaci칩n\n",
    "    texto = re.sub(r'[.,;]', '', texto)\n",
    "    # Elimina n칰meros\n",
    "    # texto = re.sub(r'\\d+', '', texto)\n",
    "    # Elimina caracteres especiales\n",
    "    texto = re.sub(r'[^\\w\\s]', '', texto)\n",
    "    # Elimina espacios dobles y saltos de l칤nea\n",
    "    texto = re.sub(r'\\s+', ' ', texto)\n",
    "    # Quita espacios al inicio y al final\n",
    "    texto = texto.strip()\n",
    "    # doc = nlp(texto)\n",
    "    return texto\n",
    "\n",
    "df['texto_limpio'] = df['productName'].apply(lambda x: limpiar_texto(x))\n",
    "\n",
    "# Mostrar las primeras filas\n",
    "df.head(3)"
